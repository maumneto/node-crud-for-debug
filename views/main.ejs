<main>
    <% for( let index = 0; index < myevents.length; index++ ) { %>
        <div class="card">
            <div class="card-header">
                <h2><%= myevents[index].title %></h2>
                <h3><%= myevents[index].date %></h3>
            </div>

            <div class="card-body">
                <p><%= myevents[index].description %></p>
                <img src="/public/assets/image-not-found.jpg" height="300px" width="300px" alt="image not image-not-founds">
            </div>

            <div class="card-footer">
                <p>Time: <%= myevents[index].time %> hour(s)</p>
                <p>Location: <%= myevents[index].location %></p>
                <p>$<%= myevents[index].price %></p>
            </div>
        </div>
    <% } %>
</main>