<main>
    <% for( let index = 0; index < myevents.length; index++ ) { %>
        <div class="card">
            <div class="card-header">
                <h2><%= myevents[index].title %></h2>
                <h3><%= myevents[index].date %></h3>
            </div>

            <div class="card-body">
                <p><%= myevents[index].description %></p>
            </div>

            <div class="card-footer">
                <p><%= myevents[index].time %></p>
                <p><%= myevents[index].location %></p>
                <p><%= myevents[index].price %></p>
            </div>
        </div>
    <% } %>
</main>